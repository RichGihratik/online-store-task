{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current",
          "browsers": ["last 2 versions", "ie >= 11"],
          "chrome": "100"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@vue/babel-sugar-composition-api-inject-h",
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "version": "7.0.0-beta.0"
      }
    ]
  ]
}
